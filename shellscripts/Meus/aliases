# Meus aliases
alias c="clear"
alias maiores='du -cks * | sort -rn | head'
alias cw='cd /backup/usb/www'
alias rc="nano /home/ribafs/.bashrc"
alias rcs="source /home/ribafs/.bashrc"
alias apti="sudo update;sudo upgrade -y;sudo apt install -y"
alias aptr="sudo apt remove --purge -y"
alias apts="sudo aptitude search"
alias p74="sudo update-alternatives --set php /usr/bin/php7.4;sudo a2dismod php8.3 php8.2;sudo a2enmod php7.4;sudo service apache2 restart;"
alias p82="sudo update-alternatives --set php /usr/bin/php8.2;sudo a2dismod php8.3 php7.4;sudo a2enmod php8.2;sudo service apache2 restart;"
alias p83="sudo update-alternatives --set php /usr/bin/php8.3;sudo a2dismod php8.2 php7.4;sudo a2enmod php8.3;sudo service apache2 restart;"
alias s="ssh -p 65522 ribafs@ribamar.net.br"
alias r="ssh -p 65522 ribafs@ribahost.net.br"
alias apr="sudo service apache2 restart"
alias phpi74="sudo nano /etc/php/7.4/apache2/php.ini;apr"
alias phpi82="sudo nano /etc/php/8.2/apache2/php.ini;apr"
alias phpi83="sudo nano /etc/php/8.3/apache2/php.ini;apr"
alias sshkey="ssh-keygen -t rsa -b 4096 -C 'ribafs2@gmail.com'"
alias sshget="cat ~/.ssh/id_rsa.pub"
alias up="sudo apt update;sudo apt upgrade -y"
alias ni="npm install;npm run build"
alias ci="composer install"
alias apti="up;sudo apt install -y"
alias laravel="/home/ribafs/.config/composer/vendor/bin/laravel"
alias l11="composer create-project --prefer-dist laravel/laravel"
alias l10="composer create-project --prefer-dist laravel/laravel:^10.0"
alias l9="composer create-project --prefer-dist laravel/laravel:^9.0"
alias as="php artisan serve"
alias am="php artisan migrate"
alias ams="php artisan migrate:fresh --seed"
alias ak="php artisan key:generate --ansi"
alias a="php artisan"
alias ce="cp .env.example .env"
alias lc="php artisan cache:composer du;clear;php artisan config:cache;php artisan route:cache;php artisan optimize"
alias ne="nano .env"
alias lr="mysql -uroot -e 'drop database laravel';mysql -uroot -e 'create database laravel';echo database laravel recreated;"
alias rr="ssh -oStrictHostKeyChecking=no -oUserKnownHostsFile=/dev/null root@ribahost.net.br"
alias rs="ssh -oStrictHostKeyChecking=no -oUserKnownHostsFile=/dev/null root@ribamar.net.br"
alias p="php -S localhost:8000"
alias rclear="php artisan route:clear"
